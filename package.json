{
  "name": "angular-ui",
  "version": "1.0.0-6",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "postpublish": "",
    "publish": "npm run prepublishOnly && npm run prepare && cd dist/angular-ui && npm publish && cd ../../ && npm run postpublish",
    "version": "npm run format",
    "preversion": "npm run lint",
    "postversion": "node sync-version.js && git push && git push --tags",
    "format": "prettier --write \"projects/showcase/src/**/*.ts\" \"projects/angular-ui/src/**/*.ts\"",
    "ng": "ng",
    "start": "ng serve showcase",
    "build": "npm run build:au && ng build showcase --base-href /showcase/",
    "watch": "ng build showcase --watch --configuration development",
    "build:au": "ng run angular-ui:build && npx tailwindcss -c projects/angular-ui/tailwind.config.js -i projects/angular-ui/src/tailwind.scss -o dist/angular-ui/style.css && node postbuild.js",
    "dev:au": "ng run angular-ui:build --watch",
    "test:watch": "ng test angular-ui",
    "test": "ng test angular-ui --code-coverage --watch=false",
    "lint": "ng lint --fix",
    "release:prerelease": "npm version prerelease && npm run publish",
    "release:prepatch": "npm version prepatch && npm run publish",
    "release:patch": "npm version patch && npm run publish",
    "release:preminor": "npm version preminor && npm run publish",
    "release:minor": "npm version monor && npm run publish",
    "release:premajor": "npm version premajor && npm run publish",
    "release:major": "npm version major && npm run publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@types/lodash": "^4.14.191",
    "lodash": "^4.17.21",
    "ngx-highlightjs": "^7.0.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular-eslint/builder": "15.0.0",
    "@angular-eslint/eslint-plugin": "15.0.0",
    "@angular-eslint/eslint-plugin-template": "15.0.0",
    "@angular-eslint/schematics": "15.0.0",
    "@angular-eslint/template-parser": "15.0.0",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.23.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.0.0",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.8.2"
  }
}
